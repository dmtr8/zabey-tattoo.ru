extends include/layout
append sharedVars
    - const pageUrl = '/';
    - const pageTitle = 'Главная';
    - const bodyAdditionClass = '';
    - const pageAdditionClass = 'main-page';
block content
    div.exempl
        section.inner
            div.meeting-block
                div.meeting-block__text-content
                    h1.meeting-block__heading.text-item.text-item--animation Zабей
                    p.meeting-block__title.text-item.text-item--animation Студия татуировки в Туле
                    a(href='about.html')
                        button.meeting-block__button О НАС
                div.meeting-block__visual
                    //<iframe id="64375027-9291-48b7-b3e5-2929131287d3" src="https://www.vectary.com/viewer/v1/?model=64375027-9291-48b7-b3e5-2929131287d3&env=cedarbridge&showPreloader=0&showInteractionPrompt=0" frameborder="0" width="100%" height="480"></iframe>
    section.inner
        h2 УСЛУГИ
        div.container
            div.row
                div.col
                    div.services
                        p.services__title БЕСПЛАТНАЯ КОНСУЛЬТАЦИЯ
                        p.services__title.services__title--mobile КОНСУЛЬТАЦИЯ
                        a(href='/')
                            div.block-decor
                                div.bg-hover
                                img(src=require("../images/index/consultation.svg"))
                div.col
                    div.services
                        p.services__title ИНДИВИДУАЛЬНЫЙ ЭСКИЗ
                        p.services__title.services__title--mobile ЭСКИЗ
                        a(href='/')
                            div.block-decor
                                div.bg-hover
                                img(src=require("../images/index/sketch.svg"))
                div.col
                    div.services
                        p.services__title ХУДОЖЕСТВЕННАЯ ТАТУИРОВКА
                        p.services__title.services__title--mobile ТАТУИРОВКА
                        a(href='/')
                            div.block-decor
                                div.bg-hover
                                img(src=require("../images/index/tattoo.svg"))
        h2 МАСТЕРА
        section.masters
                div.master-card
                    div.master
                        img.master__photo(src=require("../images/index/osipova.jpg"))
                        div.master__wrapper
                            p.master__name Ксения
                            p.master__specialty цветная татуировка
                            a(href='/')
                                button.master__button ПОДРОБНЕЕ
                    div.master
                        img.master__photo(src=require("../images/index/kovaleva.jpg"))
                        div.master__wrapper
                            p.master__name Мария
                            p.master__specialty графика
                            a(href='/')
                                button.master__button ПОДРОБНЕЕ
                    div.master
                        img.master__photo(src=require("../images/index/volkova.jpg"))
                        div.master__wrapper
                            p.master__name Ксения
                            p.master__specialty черно-белый реализм
                            a(href='/')
                                button.master__button ПОДРОБНЕЕ
        h2 ПОСЛЕДНИЕ РАБОТЫ
        div.container
            div.row
                div.col-10.mx-auto
                    div#slider.carousel.carousel-dark.slide(data-ride="carousel")
                        ol.carousel-indicators
                            li.active(data-target="#slider" data-slide-to="0")
                            li(data-target="#slider" data-slide-to="1")
                            li(data-target="#slider" data-slide-to="2")
                            li(data-target="#slider" data-slide-to="3")
                            li(data-target="#slider" data-slide-to="4")
                            li(data-target="#slider" data-slide-to="5")
                            li(data-target="#slider" data-slide-to="6")
                            li(data-target="#slider" data-slide-to="7")
                            li(data-target="#slider" data-slide-to="8")
                        div.carousel-inner
                            div.carousel-item.active
                                img.img-slider.d-block.mx-auto(src=require("../images/lastwork/a.jpg"))
                                div.carousel-caption.d-none.d-md-block
                                    h5 Ксения Осипова
                            div.carousel-item
                                img.img-slider.d-block.mx-auto(src=require("../images/lastwork/b.jpg"))
                                div.carousel-caption.d-none.d-md-block
                                    h5 Мария Ковалева
                            div.carousel-item
                                img.img-slider.d-block.mx-auto(src=require("../images/lastwork/c.jpg"))
                                div.carousel-caption.d-none.d-md-block
                                    h5 Ксения Волкова
                            div.carousel-item
                                img.img-slider.d-block.mx-auto(src=require("../images/lastwork/d.jpg"))
                                div.carousel-caption.d-none.d-md-block
                                    h5 Ксения Осипова
                            div.carousel-item
                                img.img-slider.d-block.mx-auto(src=require("../images/lastwork/e.jpg"))
                                div.carousel-caption.d-none.d-md-block
                                    h5 Мария Ковалева
                            div.carousel-item
                                img.img-slider.d-block.mx-auto(src=require("../images/lastwork/f.jpg"))
                                div.carousel-caption.d-none.d-md-block
                                    h5 Ксения Волкова
                            div.carousel-item
                                img.img-slider.d-block.mx-auto(src=require("../images/lastwork/g.jpg"))
                                div.carousel-caption.d-none.d-md-block
                                    h5 Ксения Осипова
                            div.carousel-item
                                img.img-slider.d-block.mx-auto(src=require("../images/lastwork/h.jpg"))
                                div.carousel-caption.d-none.d-md-block
                                    h5 Мария Ковалева
                            div.carousel-item
                                img.img-slider.d-block.mx-auto(src=require("../images/lastwork/b.jpg"))
                                div.carousel-caption.d-none.d-md-block
                                    h5 Ксения Волкова
                        a.carousel-control-prev(href='#slider' role="button" data-slide="prev")
                            span.carousel-control-prev-icon(aria-hidden="true")
                            span.sr-only Previous
                        a.carousel-control-next(href='#slider' role="button" data-slide="next")
                            span.carousel-control-next-icon(aria-hidden="true")
                            span.sr-only Next
        h2 КОНТАКТЫ
        section.contact-wrap
            div.contact__information-field
                div.wrap-content-box.wrap-content-box--margin-mobile
                    div.contact-content
                        img.contact-content__svg(src=require("../images/index/phone.svg"))
                        p.contact-content__title +7 (950) 904-34-25
                    div.contact-content
                        img.contact-content__svg(src=require("../images/index/map.svg"))
                        p.contact-content__title.contact-content__title--margin-top Тула, ул.Демидовская #[br] (Заречье), дом 74
                div.working-hours.working-hours--margin-mobile
                    p.working-hours__title Время работы :
                    div.working-hours__box
                        p.working-hours__title.working-hours__title--color Пн-Пт:
                        p.contact-content__title 10:00 - 20:00
                    div.working-hours__box
                        p.working-hours__title.working-hours__title--color Сб-Вс:
                        p.contact-content__title Выходной
                div.map-social-media.map-social-media--margin-mobile
                    p.working-hours__title Мы в социальных сетях :
                    div.social-media
                        a(href='https://www.instagram.com/zabey_tattoostudio/' target="_blank")
                            img.social-media__img(src=require("../images/index/instagram.svg"))
                        a(href='https://vk.com/zabey_tattoostudio' target="_blank")
                            img.social-media__img(src=require("../images/index/vk.svg"))
            div.contact-wrap__map-field
                div.contact-wrap__map
                    <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d2332.883084943039!2d37.61873731587154!3d54.217557980170874!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x413440964d1361c3%3A0x222dfb06b4a302e4!2z0JTQtdC80LjQtNC-0LLRgdC60LDRjyDRg9C7LiwgNzQsINCi0YPQu9CwLCDQotGD0LvRjNGB0LrQsNGPINC-0LHQuy4sIDMwMDA1Nw!5e0!3m2!1sru!2sru!4v1616870716708!5m2!1sru!2sru" width="800" height="600" style="border:0;" allowfullscreen="" loading="lazy"></iframe>


